@model List<Mvc_Start.Models.Student>

    @{
        ViewBag.Title = "Index";
    }

    <html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>Index</title>
    </head>
    <body>
        <h2>List of Students</h2>
        <ul>

            @foreach (var s in Model)
            {
                <li> @s.Name</li>
                <li> @s.Email</li>
                <li> @s.Age</li>
            }

        </ul>
    </body>
</html>

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
